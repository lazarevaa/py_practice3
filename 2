m = float(input("Enter magnitude value: "))
if m < 2:
    print("micro")
elif m < 3:
    print("very minor")
elif m < 4:
    print("minor")
elif m < 5:
    print("light")
elif m < 6:
    print("moderate")
elif m < 7:
    print("strong")
elif m < 8:
    print("major")
elif m < 10:
    print("great")
elif m >= 10:
    print("meteoric")
